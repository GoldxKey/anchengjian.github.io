{
  "name": "react-webpack",
  "version": "2.0.0",
  "description": "这次是大版本升级，用vue重构了整个博客",
  "main": "app/app.js",
  "scripts": {
    "dev": "export NODE_ENV=dev && webpack-dev-server --hot --devtool --config ./config/app.webpack.config.js --content-base ./ --host 0.0.0.0 --inline",
    "deploy": "export NODE_ENV=deploy && webpack -p --config ./config/app.webpack.config.js",
    "vendor": "webpack -p --config ./config/dll.webpack.config.js",
    "list": "node ./config/list.js",
    "test": "node test"
  },
  "author": "anchengjian@gmail.com",
  "license": "ISC",
  "dependencies": {
    "highlight.js": "^9.5.0",
    "html-withimg-loader": "^0.1.15",
    "marked": "^0.3.5",
    "vue": "^1.0.25",
    "vue-router": "^0.7.13",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.0.1",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.5.2",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
